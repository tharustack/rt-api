{
  "version": 2,
  "builds": [
    {
      "src": "api/rating.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/index.html",
      "dest": "public/index.html"
    },
    {
      "src": "/api/rating",
      "dest": "/api/rating.py"
    }
  ]
}
